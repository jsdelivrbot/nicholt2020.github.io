{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "noImplicitAny": false,
        "allowJs": false,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "plugins",
        "platforms",
        "node_modules",
        "hooks",
        "jspm_packages",
        "www/dist",
        "jspm_packages",
        "wwww/dist"
    ]
}